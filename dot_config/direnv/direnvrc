if [[ -f "$HOME/.config/zsh/functions/path_add_dirs" ]]; then
  path_add_dirs() {
    eval "$(cat $HOME/.config/zsh/functions/path_add_dirs)"
  }
fi

if [[ -f "$HOME/.config/zsh/functions/path_add_latest_package" ]]; then
  path_add_latest_package() {
    eval "$(cat $HOME/.config/zsh/functions/path_add_latest_package)"
  }
fi
